{
  "name": "@instagram-clone/client",
  "private": true,
  "version": "1.0.0",
  "scripts": {
    "dev": "next -p 3030",
    "build": "rimraf .next && next build",
    "gen": "graphql-codegen -r dotenv/config --config codegen.yml && prettier --write './src/generated/graphql.tsx'",
    "start": "next start -p 3030",
    "type-check": "tsc --pretty --noEmit",
    "format": "prettier --write .",
    "lint": "eslint . --ext ts --ext tsx --ext js",
    "test": "jest",
    "test-all": "yarn lint && yarn type-check && yarn test",
    "echo": "echo testing server with: $npm_package_version"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "pre-push": "yarn run type-check"
    }
  },
  "lint-staged": {
    "*.@(ts|tsx)": [
      "yarn lint",
      "yarn format"
    ]
  },
  "dependencies": {
    "@apollo/client": "^3.3.20",
    "@apollo/link-context": "^2.0.0-beta.3",
    "@chakra-ui/icons": "^1.0.13",
    "@chakra-ui/react": "^1.6.4",
    "@chakra-ui/system": "1.0.0",
    "@chakra-ui/theme": "^1.9.1",
    "@emotion/react": "^11.4.0",
    "@emotion/styled": "^11.3.0",
    "@linaria/babel-preset": "^3.0.0-beta.4",
    "@linaria/core": "^3.0.0-beta.4",
    "@linaria/react": "^3.0.0-beta.4",
    "@linaria/shaker": "^3.0.0-beta.4",
    "@zeit/next-css": "^1.0.1",
    "body-scroll-lock": "^3.1.5",
    "emotion-theming": "10.0.0",
    "formik": "^2.2.9",
    "framer-motion": "^4.1.17",
    "linaria": "^2.1.0",
    "next": "11.0.0",
    "next-linaria": "^0.10.0",
    "next-seo": "^4.24.0",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "react-dropzone": "^11.3.2",
    "react-icons": "^4.2.0",
    "react-measure": "^2.5.2",
    "react-modal": "^3.14.3"
  },
  "devDependencies": {
    "@babel/core": "^7.13.10",
    "@graphql-codegen/cli": "^1.21.5",
    "@graphql-codegen/typescript": "^1.22.2",
    "@graphql-codegen/typescript-operations": "^1.18.1",
    "@graphql-codegen/typescript-react-apollo": "^2.2.6",
    "@linaria/webpack-loader": "3.0.0-beta.4",
    "@testing-library/react": "^11.2.7",
    "@types/cookie": "^0.4.0",
    "@types/jest": "^26.0.23",
    "@types/node": "^15.6.1",
    "@types/react": "^17.0.8",
    "@typescript-eslint/eslint-plugin": "^4.25.0",
    "@typescript-eslint/parser": "^4.25.0",
    "babel-jest": "^27.0.1",
    "eslint": "^7.27.0",
    "eslint-config-next": "^11.0.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-react": "^7.23.2",
    "husky": "^6.0.0",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^27.0.1",
    "jest-watch-typeahead": "^0.6.4",
    "lint-staged": "^11.0.0",
    "prettier": "^2.3.0",
    "rimraf": "^3.0.2",
    "typescript": "^4.3.2"
  },
  "license": "MIT"
}
