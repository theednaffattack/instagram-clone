{
  "name": "@instagram-clone/client",
  "private": true,
  "version": "1.0.0",
  "workspaces": {
    "nohoist": [
      "@urql/exchange-auth"
    ]
  },
  "scripts": {
    "dev": "nodemon",
    "analyze": "cross-env ANALYZE=true yarn build:next",
    "build:next": "yarn rimraf .next && next build",
    "build:server": "yarn rimraf dist && tsc --project tsconfig.server.json",
    "build:all": "NODE_OPTIONS=--max_old_space_size=4096 && yarn build:server && yarn build:next",
    "gen": "graphql-codegen -r dotenv/config --config codegen.yml && prettier --write './src/generated/graphql.tsx'",
    "start": "cross-env NODE_ENV=production PORT=3030 node dist/server.js",
    "start-local": "cross-env NODE_ENV=development PORT=3030 node dist/server.js",
    "type-check": "tsc --pretty --noEmit",
    "format": "prettier --write .",
    "lint": "eslint . --ext ts --ext tsx --ext js",
    "test": "jest",
    "test-all": "yarn lint && yarn type-check && yarn test",
    "echo": "echo testing server with: $npm_package_version"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "pre-push": "yarn run type-check"
    }
  },
  "lint-staged": {
    "*.@(ts|tsx)": [
      "yarn lint",
      "yarn format"
    ]
  },
  "dependencies": {
    "@apollo/client": "^3.3.20",
    "@apollo/link-context": "^2.0.0-beta.3",
    "@chakra-ui/icons": "^1.0.13",
    "@chakra-ui/react": "^1.6.4",
    "@chakra-ui/system": "1.0.0",
    "@chakra-ui/theme": "^1.9.1",
    "@emotion/react": "^11.4.0",
    "@emotion/styled": "^11.3.0",
    "@linaria/babel-preset": "^3.0.0-beta.4",
    "@linaria/core": "^3.0.0-beta.4",
    "@linaria/react": "^3.0.0-beta.4",
    "@linaria/shaker": "^3.0.0-beta.4",
    "@next/bundle-analyzer": "^11.0.1",
    "@urql/core": "^2.3.1",
    "@urql/devtools": "^2.0.3",
    "@urql/exchange-auth": "^0.1.6",
    "@urql/exchange-graphcache": "^4.3.0",
    "@urql/exchange-retry": "^0.2.1",
    "@zeit/next-css": "^1.0.1",
    "apollo-link-token-refresh": "^0.3.2",
    "aws-sdk": "^2.948.0",
    "axios": "^0.21.1",
    "body-parser": "^1.19.0",
    "body-scroll-lock": "^3.1.5",
    "cookie": "^0.4.1",
    "cookies": "^0.8.0",
    "cross-env": "^7.0.3",
    "dotenv": "^10.0.0",
    "emotion-theming": "10.0.0",
    "express": "^4.17.1",
    "formik": "^2.2.9",
    "framer-motion": "^4.1.17",
    "graphql": "^15.5.1",
    "http-proxy-middleware": "^2.0.1",
    "internal-ip": "^6.2.0",
    "isomorphic-unfetch": "^3.1.0",
    "jsonwebtoken": "^8.5.1",
    "jwt-decode": "^3.1.2",
    "linaria": "^2.1.0",
    "next": "11.0.0",
    "next-compose-plugins": "^2.2.1",
    "next-cookies": "^2.0.3",
    "next-linaria": "1.0.0-beta",
    "next-seo": "^4.24.0",
    "next-urql": "^3.2.0",
    "node-jose-tools": "^1.6.28",
    "pino": "^6.12.0",
    "pino-pretty": "^5.1.2",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "react-dropzone": "^11.3.2",
    "react-icons": "^4.2.0",
    "react-measure": "^2.5.2",
    "react-modal": "^3.14.3",
    "subscriptions-transport-ws": "^0.9.19",
    "urql": "^2.0.4",
    "uuid": "^8.3.2"
  },
  "devDependencies": {
    "@babel/core": "^7.13.10",
    "@graphql-codegen/cli": "^1.21.5",
    "@graphql-codegen/typescript": "^1.22.2",
    "@graphql-codegen/typescript-operations": "^1.18.1",
    "@graphql-codegen/typescript-react-apollo": "^2.2.6",
    "@graphql-codegen/typescript-urql": "^3.0.0",
    "@linaria/webpack-loader": "3.0.0-beta.4",
    "@testing-library/react": "^11.2.7",
    "@types/body-parser": "^1.19.1",
    "@types/body-scroll-lock": "^3.1.0",
    "@types/cookie": "^0.4.0",
    "@types/express": "^4.17.13",
    "@types/jest": "^26.0.23",
    "@types/node": "^15.6.1",
    "@types/pino": "^6.3.11",
    "@types/react": "^17.0.8",
    "@types/react-measure": "^2.0.7",
    "@types/react-modal": "^3.12.1",
    "@types/uuid": "^8.3.0",
    "@typescript-eslint/eslint-plugin": "^4.25.0",
    "@typescript-eslint/parser": "^4.25.0",
    "babel-jest": "^27.0.1",
    "eslint": "^7.27.0",
    "eslint-config-next": "^11.0.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-react": "^7.23.2",
    "eslint-plugin-react-hooks": "^4.2.0",
    "husky": "^6.0.0",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^27.0.1",
    "jest-watch-typeahead": "^0.6.4",
    "lint-staged": "^11.0.0",
    "prettier": "^2.3.0",
    "rimraf": "^3.0.2",
    "typescript": "^4.3.2"
  },
  "license": "MIT"
}
