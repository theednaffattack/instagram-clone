{
  "name": "@instagram-clone/server",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "start": "node -r dotenv/config dist/main.js",
    "dt": "dt ",
    "rm-build": "yarn rimraf dist",
    "dev": "nodemon -r dotenv/config --watch 'src/**/*.ts' --exec 'ts-node' src/main.ts",
    "build": "tsc --build tsconfig.json",
    "test": "jest",
    "test-watch": "npx jest --watch",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "echo": "echo testing server with: $npm_package_version",
    "push-image": "./push-to-dockerhub.sh",
    "postbuild": "cpy --cwd=src --parents '**/*.json' ../dist/",
    "spinup": "yarn rm-build && yarn build && docker-compose -f docker-compose.dev.yml up -d --build ic-api-server",
    "spindown": "docker-compose -f docker-compose.dev.yml down",
    "c-dev": "yarn rm-build && yarn build && docker-compose -f docker-compose.dev-not-server.yml up -d && yarn dev"
  },
  "dependencies": {
    "aggregate-error": "^4.0.0",
    "apollo-errors": "^1.9.0",
    "apollo-server-express": "^2.25.0",
    "aws-sdk": "^2.918.0",
    "bcryptjs": "^2.4.3",
    "class-validator": "0.12.0",
    "colors": "^1.4.0",
    "connect-redis": "^5.2.0",
    "convict": "^6.1.0",
    "convict-format-with-validator": "^6.0.1",
    "cookie-parser": "~1.4.4",
    "cors": "^2.8.5",
    "date-fns": "2.22.1",
    "debug": "~2.6.9",
    "dotenv": "^10.0.0",
    "express": "^4.17.1",
    "express-session": "^1.17.2",
    "graphql": "^15.5.0",
    "graphql-redis-subscriptions": "^2.4.0",
    "graphql-relay": "0.6.0",
    "graphql-subscriptions": "^1.2.1",
    "http": "^0.0.1-security",
    "http-errors": "~1.6.3",
    "internal-ip": "^6.2.0",
    "nodemailer": "^6.6.2",
    "pg": "^8.6.0",
    "pino": "^6.11.3",
    "pino-pretty": "^5.0.1",
    "postmark": "^2.7.4",
    "type-graphql": "^1.1.1",
    "typeorm": "^0.2.32",
    "uuid": "^8.3.2",
    "webpack": "5",
    "zapatos": "^4.0.1"
  },
  "devDependencies": {
    "@types/bcryptjs": "^2.4.2",
    "@types/connect-redis": "^0.0.16",
    "@types/convict": "^6.0.2",
    "@types/convict-format-with-validator": "^6.0.2",
    "@types/cookie-parser": "^1.4.2",
    "@types/express-session": "^1.17.3",
    "@types/graphql-relay": "0.6.0",
    "@types/jest": "^26.0.23",
    "@types/node": "^15.6.1",
    "@types/nodemailer": "^6.4.2",
    "@types/pino": "^6.3.8",
    "@types/uuid": "^8.3.0",
    "@typescript-eslint/eslint-plugin": "^4.25.0",
    "@typescript-eslint/parser": "^4.25.0",
    "cpy-cli": "^3.1.1",
    "dokku-toolbelt": "^1.1.3",
    "eslint": "^7.27.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-config-standard": "^16.0.3",
    "eslint-plugin-import": "^2.23.3",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^3.4.0",
    "eslint-plugin-promise": "^5.1.0",
    "jest": "^27.0.3",
    "nodemon": "^2.0.7",
    "prettier": "^2.3.0",
    "standard-version": "^9.3.0",
    "ts-jest": "^27.0.1",
    "ts-node": "^10.0.0",
    "typescript": "^4.3.2"
  }
}
